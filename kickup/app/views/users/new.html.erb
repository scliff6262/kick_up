<h1>Sign Up</h1>
<%=form_for @user do |f|%>
<%=f.label :first_name%><br>
<%=f.text_field :first_name%><br></br>
<%=f.label :last_name%><br>
<%=f.text_field :last_name%><br></br>
<%=f.label :zip_code%><br>
<%=f.text_field :zip_code%><br></br>
<%=f.label :email%><br>
<%=f.text_field :email%><br></br>
<%=f.label :phone%><br>
<%=f.text_field :phone%><br></br>
<%=f.label "Playing Level"%><br>
<%= radio_button_tag("user[level]", "1") %>
<%= label_tag("Beginner") %><br>
<%= radio_button_tag("user[level]", "2") %>
<%= label_tag("Decent Player")%><br>
<%= radio_button_tag("user[level]", "3") %>
<%= label_tag("Veteran") %><br>
<%= radio_button_tag("user[level]", "4") %>
<%= label_tag("Highly Competitive")%>
<br></br>
<%=f.submit%>
<%end%>
